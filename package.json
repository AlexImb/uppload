{
  "name": "uppload",
  "version": "2.0.0-beta.1",
  "description": "Uppload is a JavaScript widget for better file uploading on the web.",
  "keywords": [
    "uppload",
    "typescript",
    "css",
    "scss",
    "el nino",
    "file upload",
    "image upload",
    "crop",
    "uploader",
    "upload",
    "javascript"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "repository": "https://github.com/elninotech/uppload",
  "author": "El Ni√±o <https://elnino.tech>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "build-site": "site",
    "build-demo": "parcel build demo/index.html",
    "demo": "parcel demo/index.html",
    "lint": "prettier '**/*.{ts,scss}' --write",
    "test-report": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "changelog": "gitmoji-changelog"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "devDependencies": {
    "@staart/site": "^1.7.14",
    "@types/jest": "^24.0.23",
    "coveralls": "^3.0.7",
    "cssnano": "^4.1.10",
    "gitmoji-changelog": "^2.0.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "parcel": "^1.12.4",
    "prettier": "^1.18.2",
    "sass": "^1.23.3",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "cropperjs": "^1.5.6",
    "jslib-html5-camera-photo": "^3.1.3",
    "mitt": "^1.2.0",
    "show-hide": "^0.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && git add ."
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/tests/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  }
}
